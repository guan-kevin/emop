<lifecycles xmlns="http://maven.apache.org/LIFECYCLE/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/LIFECYCLE/1.0.0 http://maven.apache.org/xsd/lifecycle-1.0.0.xsd">
  <lifecycle>
    <id>rps</id>
    <phases>
      <phase>
        <id>process-test-classes</id>
        <executions>
          <execution>
            <goals>
              <goal>monitor</goal>
            </goals>
          </execution>
        </executions>
        <configuration/>
      </phase>
      <phase>
        <id>test</id>
      </phase>
    </phases>
  </lifecycle>
  <lifecycle>
    <id>rts</id>
    <phases>
      <phase>
        <id>process-test-classes</id>
        <executions>
          <execution>
            <goals>
              <goal>monitor</goal>
            </goals>
          </execution>
        </executions>
        <configuration/>
      </phase>
    </phases>
  </lifecycle>
  <lifecycle>
    <id>vms</id>
    <phases>
      <phase>
        <id>process-test-classes</id>
        <executions>
          <execution>
            <goals>
              <goal>clean</goal>
            </goals>
            <configuration>
              <mode>VIOLATION_COUNTS</mode>
            </configuration>
          </execution>
        </executions>
      </phase>
      <phase>
        <id>test</id>
        <!--
        <configuration>
          <skipTests>true</skipTests>
        </configuration>
        -->
      </phase>
    </phases>
  </lifecycle>
  <lifecycle>
    <id>rpp</id>
    <phases>
      <phase>
        <id>process-test-classes</id>
        <executions>
          <execution>
            <goals>
              <goal>rpp-handler</goal>
            </goals>
          </execution>
        </executions>
      </phase>
      <phase>
        <id>test</id>
      </phase>
    </phases>
  </lifecycle>
  <lifecycle>
    <id>rps-rpp</id>
    <phases>
      <phase>
        <id>process-test-classes</id>
        <executions>
          <execution>
            <goals>
              <goal>monitor</goal>
              <goal>rpp-handler</goal>
            </goals>
            <configuration>
              <rpsRpp>true</rpsRpp>
            </configuration>
          </execution>
        </executions>
      </phase>
      <phase>
        <id>test</id>
      </phase>
    </phases>
  </lifecycle>
  <lifecycle>
    <id>rpp-vms</id>
    <phases>
      <phase>
        <id>process-test-classes</id>
        <executions>
          <execution>
            <goals>
              <goal>rpp-handler</goal>
            </goals>
          </execution>
          <execution>
            <goals>
              <goal>clean</goal>
            </goals>
            <configuration>
              <mode>VIOLATION_COUNTS</mode>
            </configuration>
          </execution>
        </executions>
      </phase>
      <phase>
        <id>test</id>
      </phase>
    </phases>
  </lifecycle>
  <lifecycle>
    <id>rps-rpp-vms</id>
    <phases>
      <phase>
        <id>process-test-classes</id>
        <executions>
          <execution>
            <goals>
              <goal>monitor</goal>
              <goal>rpp-handler</goal>
            </goals>
            <configuration>
              <rpsRpp>true</rpsRpp>
            </configuration>
          </execution>
          <execution>
            <goals>
              <goal>clean</goal>
            </goals>
            <configuration>
              <mode>VIOLATION_COUNTS</mode>
            </configuration>
          </execution>
        </executions>
      </phase>
      <phase>
        <id>test</id>
      </phase>
    </phases>
  </lifecycle>
  <lifecycle>
    <id>rps-vms</id>
    <phases>
      <phase>
        <id>process-test-classes</id>
        <executions>
          <execution>
            <goals>
              <goal>monitor</goal>
            </goals>
          </execution>
          <execution>
            <goals>
              <goal>clean</goal>
            </goals>
            <configuration>
              <mode>VIOLATION_COUNTS</mode>
            </configuration>
          </execution>
        </executions>
      </phase>
      <phase>
        <id>test</id>
      </phase>
    </phases>
  </lifecycle>
</lifecycles>
